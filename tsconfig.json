{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "module": "CommonJS",
    "baseUrl": "src",
    "rootDir": "src",
    "moduleResolution": "node",
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "declaration": true,
    "experimentalDecorators": true,
    "paths": {
      "@kuramisa": [
        "./src/Kuramisa"
      ],
      "@dashboard": [
        "./src/dashboard/index"
      ],
      "@dashboard/*": [
        "./src/dashboard/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@struct/*": [
        "./src/struct/*"
      ],
      "@schemas/*": [
        "./src/schemas/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}